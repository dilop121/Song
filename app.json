{
  "name": "Pyrogram",
  "description": "Personal use and learn Pyrogram bot.",
  "keywords": ["telegram", "pyrogram"],
  "repository": "https://github.com/Itz-Barath/PyrogramBot",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram Session String Get From https://my.telegram.org/auth",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A Bot Token From @BotFather",
      "required": true
    },
    "BOT_USERNAME": {
      "description": "Your Bot Username Without @, Get From @BotFather",
      "required": true,
      "value": "ALIEN_MUSIC_ROBOT"
    },
    "API_ID": {
      "description": "API_ID from my.telegram.org/apps",
      "required": true,
      "value": "14621169"
    },
    "API_HASH": {
      "description": "API_HASH from my.telegram.org/apps",
      "required": true,
      "value": "61e480d82ff09acbf967ccea4fa2884f"
    },
    "OWNER_ID": {
      "description": "Your Telegram ID",
      "required": true,
      "value": "2006619406"
    }
  },
  "addons": [],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "basic"
    }
  },
  "stack": "container"
}
